<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Component1.quizapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizApplication">
        <activity android:name="com.Component1.QuizApp.ScoreActivity"></activity>
        <activity android:name="com.Component1.QuizApp.QuizActivity" />
        <activity android:name="com.Component1.QuizApp.ParticipantActivity" />
        <activity android:name="com.Component1.QuizApp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>